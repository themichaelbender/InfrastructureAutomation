<?xml version="1.0" encoding="utf-8" ?>
<Types>
  <Type>
    <Name>MOL.SystemInfo</Name>
    <Members>
      <ScriptProperty>
        <Name>NormalizedBIOSSerial</Name>
        <GetScriptBlock>
            $this.biosserial -replace ' ',''
        </GetScriptBlock>
        </ScriptProperty>
        <ScriptMethod>
          <Name>CanPing</Name>
          <Script>
            Test-Connection -ComputerName $this.ComputerName -Quiet
          </Script>
        </ScriptMethod> 
    </Members>
  </Type>
</Types>
